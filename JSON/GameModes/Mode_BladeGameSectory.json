{
	"$type": "GameModeLoader.Data.LevelDataModeCatalog, GameModeLoader",
	"id": "bladegamesectory",
	"version": 3,
	"excludeLevelIds": ["Greenland"],
	"explicitOptions":  true,
	"gameModeOptionIds": ["heavygravity", "hponkill","killslowmo","lunargravity","parryslowmo","superhot"],
	"mode": {
		"$type": "ThunderRoad.LevelData+Mode, ThunderRoad",
		"name": "Blade Game Sectory",
		"displayName": "Blade Game Sectory",
		"description": "Gun Game? Now its time for Blade Game! Melee only. Each kill levels up your weapon. Can you survive? With Sectory compatability",
		"allowGameModes": [
			"Sandbox",
			"Arcade"
		],
		"mapOrder": 1,
		"difficultyMultipliers": [],
		"modules": [
			{
				"$type": "ThunderRoad.LevelModuleCleaner, ThunderRoad",
				"cleanerRate": 5.0
			},
			{ "$type": "ThunderRoad.LevelModuleXP, ThunderRoad" },
			{ "$type": "GameModeLoader.Component.RemoveWaveBook, GameModeLoader", "enable":  true },
			{ "$type": "GameModeLoader.Component.RemoveWeaponBook, GameModeLoader", "enable":  true },
			{ "$type": "GameModeLoader.Component.RemoveWeaponRack, GameModeLoader", "enable":  true },
			{ "$type": "GameModeLoader.Component.NoSpellWheel, GameModeLoader", "enable":  true },
			{ "$type": "GameModeLoader.Component.NoTk, GameModeLoader", "enable":  true },
			{ "$type": "GameModeLoader.Component.Respawn, GameModeLoader", "enable":  true, "lives":  3, "behaviour": "ShowScores",
				"delayBeforeLoad": 10.0, "disablePlayerLocomotion": true},
			{
				"$type": "GameModeLoader.GameMode.BladeGame, GameModeLoader", "enable":  true,
				"excludeItemIds": [
					"MMPLantern", "Torch", "BactaStim", "LightsaberTool", "MMPMagicBook",
					"BlasterPowerCell_Blue","BlasterPowerCell_Cyan","BlasterPowerCell_Green",
					"BlasterPowerCell_Orange","BlasterPowerCell_Pink","BlasterPowerCell_Purple",
					"BlasterPowerCell_Red","BlasterPowerCell_Yellow"
				],
				"tierWaves":  ["BGBanditEndless", "BGGladiatorEndless", "BGKnightEndless", "BGMixedSpecial"],
				"anyKillCounts": true
			}
		],
		"availableOptions": []
	}
}